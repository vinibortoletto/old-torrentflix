flix\\src\\pages\\Landing\\OurStory\\OurStory.styles.js",[],"C:\\Users\\Vinicius\\Documents\\Code\\torrentflix\\src\\components\\Background\\Background.styles.js",["320"],"import styled, { css } from 'styled-components/macro';\r\nimport { media } from '../../helpers/styles/mixins';\r\nimport { vars } from '../../helpers/styles/variables';\r\n\r\nexport const Image = styled.img`\r\n  height: 100%;\r\n  width: 100%;\r\n\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  z-index: -1;\r\n\r\n  filter: contrast(1.1);\r\n  object-fit: cover;\r\n\r\n  /* Hero */\r\n  ${(props) =>\r\n    props.hero &&\r\n    css`\r\n      height: 80vh;\r\n\r\n      ${media('tablet')} {\r\n        height: 100vh;\r\n      }\r\n\r\n      ${media('laptop-l')} {\r\n        height: 90vh;\r\n      }\r\n\r\n      ${media('4k')} {\r\n        height: 52vh;\r\n      }\r\n    `}\r\n`;\r\n","C:\\Users\\Vinicius\\Documents\\Code\\torrentflix\\src\\pages\\Login\\Form\\Form.styles.js",["321"],"import styled from 'styled-components/macro';\r\nimport { vars } from '../../../helpers/styles/variables';\r\nimport { media } from '../../../helpers/styles/mixins';\r\n\r\nexport const Form = styled.form`\r\n  margin-bottom: 2rem;\r\n\r\n  .input-wrapper {\r\n    margin-bottom: 1rem;\r\n  }\r\n\r\n  #button-submit {\r\n    margin-top: 2rem;\r\n    margin-bottom: 1rem;\r\n    padding: 1.5rem;\r\n    width: 100%;\r\n    font-weight: bold;\r\n    font-size: 1rem;\r\n  }\r\n`;\r\n\r\nexport const Helper = styled.div`\r\n  display: flex;\r\n  justify-content: space-between;\r\n\r\n  label,\r\n  a {\r\n    font-size: ${vars.fontSize.small};\r\n    color: ${vars.color.lightGrey};\r\n  }\r\n\r\n  input {\r\n    -webkit-appearance: none;\r\n    -moz-appearance: none;\r\n    appearance: none;\r\n  }\r\n\r\n  .remember-login {\r\n    cursor: pointer;\r\n    position: relative;\r\n\r\n    &::before {\r\n      content: '';\r\n      width: 1rem;\r\n      height: 1rem;\r\n      background-color