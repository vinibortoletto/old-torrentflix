   },\r\n      {\r\n        breakpoint: 600,\r\n        settings: {\r\n          slidesToShow: 2,\r\n          slidesToScroll: 2,\r\n          initialSlide: 2,\r\n        },\r\n      },\r\n      {\r\n        breakpoint: 480,\r\n        settings: {\r\n          slidesToShow: 1,\r\n          slidesToScroll: 1,\r\n        },\r\n      },\r\n    ],\r\n  };\r\n\r\n  return (\r\n    <Container>\r\n      <Title>{list.title}</Title>\r\n\r\n      <List>\r\n        <Slider {...settings}>\r\n          {list.item.results.map((item) => (\r\n            <li key={item.id}>\r\n              {/* <HoverCard item={item} /> */}\r\n\r\n              {list.title.includes('netflix') && (\r\n                <CardIcon src={icon} alt=\"torrentflix logo\" />\r\n              )}\r\n\r\n              <CardTitle>{item.name || item.title}</CardTitle>\r\n              <CardOverlay className=\"overlay\" />\r\n              <CardImg\r\n                src={`https://image.tmdb.org/t/p/w300${item.backdrop_path}`}\r\n                alt={`${item.name} poster`}\r\n              />\r\n            </li>\r\n          ))}\r\n        </Slider>\r\n\r\n        {/* {spotlight && (\r\n          <li>\r\n            <HoverCard item={spotlight} />\r\n\r\n            {list.title.includes('netflix') && (\r\n              <CardIcon src={icon} alt=\"torrentflix logo\" />\r\n            )}\r\n\r\n            <CardTitle>{spotlight.name || spotlight.title}</CardTitle>\r\n            <CardOverlay className=\"overlay\" />\r\n            <CardImg\r\n              src={`https://image.tmdb.org/t/p/w300${spotlight.backdrop_path}`}\r\n              alt={`${spotlight.name} poster`}\r\n            />\r\n          </li>\r\n        )} */}\r\n      </List>\r\n    </Container>\r\n  );\r\n}\r\n","C:\\Users\\Vinicius\\Documents\\Code\\torrentflix\\src\\pages\\Landing\\Hero\\Hero.js",[],"C:\\Users\\Vinicius\\Documents\\Code\\torrentflix\\src\\pages\\Browse\\Spotlight\\Spotlight.js",[],"C:\\Users\\Vinicius\\Documents\\Code\\torrentflix\\sr